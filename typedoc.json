{
    "entryPoints": [
        "./src/scripts/OSFramework/*.ts",
        "./src/scripts/OutSystems/OSUI/*.ts",
        "./src/scripts/Providers/*.ts"
    ],
    "entryPointStrategy": "expand",
    "includeVersion": true,
    "hideGenerator": true,
    "out": "./docs",
    "sort": "source-order",
    "umlClassDiagram": {
        "type": "detailed",
        "location": "local",
        "format": "svg",
        "backgroundColor": "#ced4da",
        "boxBackgroundColor": "#fff",
        "boxBorderColor": "#272b30",
        "arrowColor": "#272b30"
    }
}